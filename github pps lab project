#include <stdio.h>

int main()
{
    int sub1,sub2,sub3,sub4,sub5,perc;
    printf("Hello Student \n");
    printf("Get your percentage here \n");
    printf("Enter your Physics score: \n");
    scanf("\n %d",&sub1);
    printf("Enter your Chemistry score: \n");
    scanf("\n %d",&sub2);
    printf("Enter your Maths score: \n");
    scanf("\n %d",&sub3);
    printf("Enter your CS score: \n");
    scanf("\n %d",&sub4);
    printf("Enter your English score: \n");
    scanf("\n %d",&sub5);
    perc=(sub1+sub2+sub3+sub4+sub5)/5;
    printf("Your percentage is %d \n",perc);
    
    return 0;
}
